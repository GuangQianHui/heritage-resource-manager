{
  "name": "voice-to-text-app",
  "version": "3.1.0",
  "description": "非遗文化传承智能体助手 - 语音转文字应用，包含资源管理系统",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:resources": "node resources-server/server.js",
    "dev:resources": "nodemon resources-server/server.js",
    "start:resources-modular": "node resources-server/server-modular.js",
    "dev:resources-modular": "nodemon resources-server/server-modular.js",
    "test:resources": "node resources-server/test-modular.js",
    "start:all": "node start-servers.js",
    "start:all-modular": "RESOURCE_SERVER_MODULAR=true node start-servers.js",
    "dev:all": "nodemon start-servers.js",
    "dev:all-modular": "RESOURCE_SERVER_MODULAR=true nodemon start-servers.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "voice",
    "speech",
    "AI",
    "heritage",
    "culture",
    "resource-management",
    "media-preview",
    "search",
    "export"
  ],
  "author": "Guang Qianhui",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.7.2",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
